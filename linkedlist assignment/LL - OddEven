import java.util.*;
public class Main {
    public static void main (String args[]) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int[] a =  new int[n];
		for(int i=0; i<n; i++){
			a[i] = s.nextInt();
			if(a[i]%2==1)
			{
				System.out.print(a[i]+" ");
			}
		}
		for(int i : a)
		{
			if(i%2==0) 
			System.out.print(i + " ");
		}
    }
}
