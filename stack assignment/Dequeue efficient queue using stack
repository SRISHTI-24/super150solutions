
import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner s =  new Scanner(System.in);
		int l = s.nextInt();
		Stack<Integer> sk1= new Stack<>();
		Stack<Integer> sk2= new Stack<>();
		for(int i =0;i<l;i++)
		{
			sk1.push(i);
		}
		for(int i =0;i<l;i++)
		{
			sk2.push(sk1.pop());
		}
		for(int i =0;i<l;i++)
		{
			System.out.print(sk2.pop()+" ");
		}
        // Your Code Here
    }
}
