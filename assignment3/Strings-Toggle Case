#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
	string str;
	cin>>str;
	for(int i=0;i<str.size();i++)
	{
		if(str[i]>=65&&str[i]<=91)
		{
			str[i]=str[i]+32;
		}
		else if(str[i]>=97&&str[i]<=122)
		{
			str[i]=str[i]-32;
		}
	}
	for(int i=0;i<str.size();i++)
	cout<<str[i];
	return 0;
}
